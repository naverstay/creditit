mixin header(active_menu)
    div.header
        .header_line
            .section_inner.clearfix
                .fl: .mb
                    b.slogan единый кредитный интегратор
                    
                .fr.user_menu
                    .mb
                        span Дилерский центр: СТК сервис
                    .mb
                        a.gl_link.color_violet(href='#') Выход

        .section_inner
            .main_menu_holder
                .fl
                    .mb: a.logo(href='index.html')
                        img(src='i/logo_2.svg')

                ul.fr.main_menu
                    li.mb: a.menu_link(class = (active_menu == 1 ? 'active' : '') href='new_order.html') Оформить заявку
                    li.mb: a.menu_link(class = (active_menu == 2 ? 'active' : '') href='orders.html') Заявки
                    li.mb: a.menu_link(class = (active_menu == 3 ? 'active' : '') href='#') Статистика
 

