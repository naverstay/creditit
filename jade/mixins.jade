mixin input_composer(w_cls, inp_id, inp_cls, inp_plc, inp_val, inp_disable, inp_data_format) 
    .input_w(class=w_cls)
        input.f_input(id=inp_id || null class=inp_cls placeholder=inp_plc || null value=inp_val || null disabled = (inp_disable ? 'disabled' : null) data-format = inp_data_format || null)
        

mixin select2 (first_option, id)
    select.select2(id=id)
        option(value=0)=first_option
        option(value=1) Выбор 2

